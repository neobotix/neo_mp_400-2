<?xml version="1.0" ?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="mp_400">
  <!-- SENSORS -->
  <xacro:arg name="use_imu" default="false"/>
  <xacro:arg name="use_d435" default="false"/>

  <xacro:property name="imu" value="$(arg use_imu)"/>
  <xacro:property name="d435" value="$(arg use_d435)"/>

  <link name="base_link"/>
  <joint name="base_footprint_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <axis xyz="0 0 -1"/>
    <joint_properties damping="1" friction="1"/>
    <parent link="base_link"/>
    <child link="base_footprint"/>
  </joint>
  <link name="base_footprint">
    <inertial>
      <mass value="30"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="1.71402" ixy="0.0" ixz="0.0" iyy="0.8856" iyz="0.0" izz="2.5725"/>
    </inertial>
    <visual name="body">
      <origin rpy="0 0 1.57" xyz="0.0 0.0 0."/>
      <geometry>
        <mesh filename="package://neo_mp_400-2/robot_model/mp_400/meshes/MP-400-BODY.dae" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision name="body_collision">
      <origin rpy="0 0 1.57" xyz="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="package://neo_mp_400-2/robot_model/mp_400/meshes/MP-400-BODY.dae" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>
  <joint name="mp_400_wheel_front_left_joint" type="fixed">
    <origin rpy="0 0 1.57" xyz="0.165 0.155 0.036"/>
    <axis xyz="-1 0 0"/>
    <joint_properties damping="1." friction="1.0"/>
    <limit effort="1000.0" lower="-1e+16" upper="1e+16" velocity="3.5"/>
    <parent link="base_footprint"/>
    <child link="mp_400_wheel_front_left_link"/>
  </joint>
  <link name="mp_400_wheel_front_left_link" type="wheel">
    <inertial>
      <mass value="1.4"/>
      <inertia ixx="0.00499743171" ixy="0.00000004464" ixz="-0.00000002245" iyy="0.00499741733" iyz="-0.00000000164" izz="0.00839239692"/>
    </inertial>
    <visual name="caster_wheel">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://neo_mp_400-2/robot_model/mp_400/meshes/MP-400-CASTER-WHEEL.dae" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0. 0"/>
